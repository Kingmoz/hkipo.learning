        if (pathname.indexOf("/tc/") >= 0) {
            window.location.href = '/tc/changelang.aspx?lang=' + v + '&url=' + pathname.replace("/tc/", "/" + v + "/") + escape(search);
        } else if (pathname.indexOf("/sc/") >= 0) {
