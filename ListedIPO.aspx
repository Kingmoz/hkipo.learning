    function shhkquote(s, type, market) {
        
        type = type== undefined ? 'quote' : type;
     
        if (typeof market == 'undefined') {
	        if (s.length == 6) {
	            market = 'SH';
	        } else {
                market = 'HK';
            }
        }
        
        switch (market) {
            case 'HK':
            s = '00000' + s;
            s = s.substr(s.length - 5, 5);
                break;
            case 'SH':
                break;
            case 'US':
                break;
        }
        
        switch (type) {
            case 'quote':   
                $('button[type=submit], input[type=submit]').attr('disabled', true);             
                $('form').submit(function() { return false; });
                if (market == 'SH')
                    cnquote(s);
                else if (market == 'US') 
                    usquote(s);
                else {
                    SetLTP('www.aastocks.com');
                    hkquote(s);
                }
                break;
            case 'chart':
                $('button[type=submit], input[type=submit]').attr('disabled', true);             
                if (market == 'SH') 
                    cnchart(s);
                else if (market == 'US') 
                    uschart(s);
                else
                    hkchart(s);
                break;
            case 'news':
                $('button[type=submit], input[type=submit]').attr('disabled', true);             
                if (market == 'SH') 
                    cnnews(s);
                else
                    hknewswvalue(s);
                break;
        }
        
        return false;
    }
